      <Query Active="True" DisplayList="True" DisplayStat="True" DisplaySelectionView="False" IsCriticalRule="True"><![CDATA[// <Name>Avoid using raw types</Name>
warnif count > 0 

let rawTypes = ThirdParty.Types.WithFullNameIn(
